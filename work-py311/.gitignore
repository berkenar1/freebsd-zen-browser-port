!third_party/python/**/*.egg-info
# lint-ignore-next-line: syntax-difference
!testing/web-platform/tests/tools/third_party/**/*.egg-info

# Vim swap files.
.*.sw[a-z]
.sw[a-z]

# Emacs directory variable files.
**/.dir-locals.el
# Emacs project sentinel files.
**/.projectile

